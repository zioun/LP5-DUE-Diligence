#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules/jiti/bin/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules/jiti/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules/jiti/bin/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules/jiti/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/jiti@1.21.7/node_modules:/mnt/c/Users/jionk/Downloads/Copia de LP5/lp5-due-diligence/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jiti@1.21.7/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../jiti@1.21.7/node_modules/jiti/bin/jiti.js" "$@"
fi
